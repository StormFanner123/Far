<%= simple_form_for(@team) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :image %>
    <%= f.input :average_level %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Create!", class: 'btn btn-success' %>
  </div>
<% end %>
